{
  "name": "readable-readme",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub action that makes REAME readable",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "jest && tslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "format": "tslint 'src/*.ts' 'src/**/*.ts' '__tests__/**/*.ts' '__tests__/*.ts' --fix && prettier --write 'src/*.ts' 'src/**/*.ts' '__tests__/**/*.ts' '__tests__/*.ts'",
    "docs": "typedoc --readme ./src/README.md --out ./docs/api ./src",
    "start": "LOCAL_TESTING=true ts-node ./src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tianhaoz95/readable-readme.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "JacksonZ",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/github": "^2.2.0",
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow/tfjs": "^1.7.4",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "marked": "^1.1.0",
    "micromatch": "^4.0.2",
    "mustache": "^4.0.1",
    "nock": "^12.0.3",
    "utf-8-validate": "^5.0.2",
    "write-good": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "jest": "^25.5.4",
    "jest-circus": "^26.0.1",
    "prettier": "2.0.5",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3",
    "vscode-ripgrep": "^1.5.8"
  },
  "jest": {
    "verbose": true
  }
}
